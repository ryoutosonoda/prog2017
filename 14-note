import turtle
turtle = turtle.Turtle()
turtle.position()

import turtle
t = Turtle.turtle()

t.forward(100)
t.left(90)
t.forward(100)

def rect(x,y,a):
  t.up()
  t.home()
  t.forward(x)
  t.left(90)
  t.forward(y)
  t.down()
  for i in range(4):
    t.forward(a)
    t.left(-90)

t.clear()
rect(10,20,100)
   def rect(x, y, a):
