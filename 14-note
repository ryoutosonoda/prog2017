  t.home()
  t.forward(x)
  t.left(90)
  t.forward(y)
  t.down()
  for i in range(4):
    t.forward(a)
    t.left(-90)
#空行

t.clear()

rect(10, 10, 100)
rect(10, -110, 100)
rect(-110, 10, 100)
rect(-110, -110, 100)

				
class Dice:
  face_num = 6
  def shoot(self):
    return random.randint(1,self.face_num)


important random
aDice = Dice()
aDice.shoot()



aDice = Dice() #クラスをもとにオブジェクトを作る
aDice.shoot() #オブジェクトを使う

 
