cicle(100,200,50)

def circle(x,y,r =150):
 t.up()
 t.home()
 t.forward(x)
 t.left(90)
 t.forward(y)
 t.forward(r)
 t.left(90)
 t.down()
 t.circle(r)
t.home()
t.clear()

cicle(0,0,100)
cicle(100,200,50)


def circle(x, y, r = 150):

  t.up()

  t.home()

  t.forward(x)

  t.left(90)

  t.forward(y)

  t.forward(r)

  t.left(90)

  t.down()

  t.circle(r)



t.home()

t.clear()



circle(100, -100)

circle(100, 100, 50)

circle(-100, -100, 50)

circle(-100, 100, 50)
