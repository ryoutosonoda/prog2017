important turtle()
t = turtle.Turtle()

t.home()
t.clear()

def total(n)
 s = 0
 for i in range(1,n + 1):
  s += i
 return s
 
total(5)

total(10)


import turtle
turtle = turtle.Turtle()

def center_circle():
 turtle.penup()
 turtle.forward(200)
 turtle.left(90)
 turtle.pendown()
 turtle.circle(200)
 turtle.left(90)
 turtle.penup()
 turtle.forward(200)
 turtle.pendown()
 
center_circle()


